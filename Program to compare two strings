#include<bits/stdc++.h>
using namespace std;
int main()
{
    char a[100], b[100];
    cout<<"enter the first string" <<endl;
    cin>>a;
    cout<<"enter the second string" <<endl;
    cin>>b;
    strcmp(a,b);
    if(strcmp(a,b)==0)
    {
        cout<<"Strings are equal" <<endl;
    }
    else
    {
        cout<<"Strings are not equal" <<endl;
        
        
    }
    return 0;
}

