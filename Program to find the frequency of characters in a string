#include<bits/stdc++.h>
using namespace std;
int main()
{
    char str[100];
    cout<<"enter a string" <<endl;
    cin>>str;
    int freq[256] = {0};
    int i;
    for(i=0; str[i]!='\0'; i++)
    {
        freq[str[i]]++;
    }
    for(i=0; i<256; i++)
    {
        if(freq[i]!=0)
        {
            cout<<"The frequency of" <<" " << i <<" " << "is" <<" " <<freq[i] << endl;
        }
    }
    return 0;
}

enter a string
aakriti
The frequency of 97 is 2
The frequency of 105 is 2
The frequency of 107 is 1
The frequency of 114 is 1
The frequency of 116 is 1
